{
  "name": "@cureeat/web",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "build:image": "docker build -t cureeat/web --build-context monorepo=../../ .",
    "docker:start": "docker run -p 3000:3000 cureeat/web",
    "docker:stop": "docker kill $(docker ps -a -q --filter ancestor=cureeat/web)",
    "build:image:dev": "docker build -f Dockerfile.dev -t cureeat-web-dev --build-context monorepo=../../ .",
    "docker:start:dev": "docker run -p 5173:5173 --mount type=bind,source=./,target=/home/cureeat/apps/web cureeat-web-dev",
    "docker:stop:dev": "docker kill $(docker ps -a -q --filter ancestor=cureeat-web-dev)",
    "preview": "vite preview",
    "test": "vitest --watch false",
    "test:watch": "vitest --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.27.6",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.1",
    "tailwindcss": "^3.3.5",
    "vite": "^4.0.0",
    "vitest": "^0.34.6",
    "vitest-fetch-mock": "^0.2.2"
  },
  "dependencies": {
    "fp-ts": "^2.16.1",
    "lodash-es": "^4.17.21",
    "pocketbase": "^0.19.0"
  }
}
